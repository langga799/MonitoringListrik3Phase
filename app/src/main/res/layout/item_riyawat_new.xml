<?xml version="1.0" encoding="utf-8"?><com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:layout_marginHorizontal="8dp"    android:layout_marginTop="8dp"    app:cardBackgroundColor="@color/purple_500"    app:cardCornerRadius="8dp">    <androidx.constraintlayout.widget.ConstraintLayout        android:layout_width="match_parent"        android:layout_height="match_parent">        <TextView            android:id="@+id/tvDate"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginStart="16dp"            android:layout_marginTop="8dp"            android:text="12-01-2022"            android:textColor="@color/white"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent" />        <TextView            android:id="@+id/tvFasa"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_marginStart="16dp"            android:layout_marginTop="8dp"            android:layout_marginEnd="16dp"            android:text="Fasa"            android:textAlignment="textEnd"            android:textColor="@color/white"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintHorizontal_bias="1.0"            app:layout_constraintStart_toEndOf="@+id/tvDate"            app:layout_constraintTop_toTopOf="parent" />        <TextView            android:id="@+id/textView20"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_marginStart="16dp"            android:layout_marginTop="8dp"            android:gravity="start"            android:text="Ampere"            android:textAlignment="textStart"            android:textColor="@color/white"            android:textSize="16sp"            android:textStyle="bold"            app:layout_constraintEnd_toStartOf="@+id/tvKeteranganFasa1"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/tvDate" />        <TextView            android:id="@+id/textView22"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_marginStart="16dp"            android:layout_marginTop="8dp"            android:gravity="start"            android:text="Tegangan"            android:textAlignment="textStart"            android:textColor="@color/white"            android:textSize="16sp"            android:textStyle="bold"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toStartOf="@+id/tvKeteranganFasa2"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/textView20"            app:layout_constraintVertical_bias="0.0" />        <TextView            android:id="@+id/textView23"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_marginStart="16dp"            android:layout_marginTop="8dp"            android:layout_marginBottom="16dp"            android:gravity="start"            android:text="Daya"            android:textAlignment="textStart"            android:textColor="@color/white"            android:textSize="16sp"            android:textStyle="bold"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toStartOf="@+id/tvKeteranganFasa3"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/textView22"            app:layout_constraintVertical_bias="0.0" />        <TextView            android:id="@+id/tvKeteranganFasa1"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:gravity="center_horizontal"            android:text="Stabil"            android:textAlignment="center"            android:textColor="@color/white"            android:textSize="16sp"            android:visibility="invisible"            app:layout_constraintBottom_toBottomOf="@+id/textView20"            app:layout_constraintEnd_toStartOf="@+id/tvAmpereStatus"            app:layout_constraintStart_toEndOf="@+id/textView20"            app:layout_constraintTop_toTopOf="@+id/textView20" />        <TextView            android:id="@+id/tvKeteranganFasa2"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:gravity="center_horizontal"            android:text="Stabil"            android:textAlignment="center"            android:textColor="@color/white"            android:textSize="16sp"            android:visibility="invisible"            app:layout_constraintBottom_toBottomOf="@+id/textView22"            app:layout_constraintEnd_toStartOf="@+id/tvTeganganStatus"            app:layout_constraintStart_toEndOf="@+id/textView22"            app:layout_constraintTop_toTopOf="@+id/textView22"            app:layout_constraintVertical_bias="0.0" />        <TextView            android:id="@+id/tvKeteranganFasa3"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:gravity="center_horizontal"            android:text="Stabil"            android:textAlignment="center"            android:textColor="@color/white"            android:textSize="16sp"            android:visibility="invisible"            app:layout_constraintBottom_toBottomOf="@+id/textView23"            app:layout_constraintEnd_toStartOf="@+id/tvDayaStatus"            app:layout_constraintStart_toEndOf="@+id/textView23"            app:layout_constraintTop_toTopOf="@+id/textView23"            app:layout_constraintVertical_bias="0.533" />        <TextView            android:id="@+id/tvAmpereStatus"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_marginEnd="16dp"            android:gravity="end"            android:text="100%"            android:textAlignment="textEnd"            android:textColor="@color/white"            android:textSize="16sp"            app:layout_constraintBottom_toBottomOf="@+id/tvKeteranganFasa1"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toEndOf="@+id/tvKeteranganFasa1"            app:layout_constraintTop_toTopOf="@+id/tvKeteranganFasa1" />        <TextView            android:id="@+id/tvTeganganStatus"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_marginEnd="16dp"            android:gravity="end"            android:text="100%"            android:textAlignment="textEnd"            android:textColor="@color/white"            android:textSize="16sp"            app:layout_constraintBottom_toBottomOf="@+id/tvKeteranganFasa2"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toEndOf="@+id/tvKeteranganFasa2"            app:layout_constraintTop_toTopOf="@+id/tvKeteranganFasa2" />        <TextView            android:id="@+id/tvDayaStatus"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_marginEnd="16dp"            android:gravity="end"            android:text="100%"            android:textAlignment="textEnd"            android:textColor="@color/white"            android:textSize="16sp"            app:layout_constraintBottom_toBottomOf="@+id/tvKeteranganFasa3"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toEndOf="@+id/tvKeteranganFasa3"            app:layout_constraintTop_toTopOf="@+id/tvKeteranganFasa3" />        <TextView            android:id="@+id/textView24"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_marginStart="16dp"            android:layout_marginTop="8dp"            android:layout_marginBottom="16dp"            android:text="Persentase Ketidakseimbangan"            android:textColor="@color/white"            android:textSize="16sp"            android:textStyle="bold"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintEnd_toStartOf="@+id/tvStatuesPersentase"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/textView23"            app:layout_constraintVertical_bias="0.0" />        <TextView            android:id="@+id/tvStatuesPersentase"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_marginStart="8dp"            android:layout_marginEnd="8dp"            android:gravity="center_horizontal"            android:text=""            android:textAlignment="center"            android:textColor="#FFCF1F"            android:textSize="12sp"            android:visibility="visible"            app:layout_constraintBottom_toBottomOf="@+id/textView24"            app:layout_constraintEnd_toStartOf="@+id/tvPersentase"            app:layout_constraintStart_toEndOf="@+id/textView24"            app:layout_constraintTop_toTopOf="@+id/textView24" />        <TextView            android:id="@+id/tvPersentase"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_marginEnd="16dp"            android:text="100%"            android:textAlignment="textEnd"            android:textColor="@color/white"            android:textSize="16sp"            app:layout_constraintBottom_toBottomOf="@+id/textView24"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintTop_toTopOf="@+id/textView24" />    </androidx.constraintlayout.widget.ConstraintLayout></com.google.android.material.card.MaterialCardView>